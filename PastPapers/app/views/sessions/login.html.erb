<div>
    <h1 class="title">University of Edinburgh Past Paper Share</h1>
    <%= render partial: "home/navbar"%>
    <h1>Login</h1>
    <%= form_tag "/login" do %>
        <div>
        <%= label_tag :email %>
        <%= text_field_tag :email, params[:email] %>
        </div>
        <div>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        </div>
        <div>
        <%= submit_tag "Login" %>
        </div>
    <% end %>
</div>
